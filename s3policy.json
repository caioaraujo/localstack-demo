{
  "Statement": [
    {
      "Sid": "example-statement-ID",
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": [
        "SQS:SendMessage"
      ],
      "Resource": "SQS-queue-ARN",
      "Condition": {
        "ArnLike": {
          "aws:SourceArn": "arn:aws:s3:*:*:bucket-demo"
        },
        "StringEquals": {
          "aws:SourceAccount": "${AWS_ACCESS_KEY_ID}"
        }
      }
    }
  ]
}
